{
  "globals": {
    "cy": true,
    "before": true,
    "beforeEach": true
  },
  "rules": {
    "indent": [
      1,
      2,
      { "SwitchCase": 1 }
    ],
    "quotes": [
      1,
    "single"
    ],
    "linebreak-style": [
      1,
    "unix"
    ],
    "no-console": 0,
    "react/prop-types": 1,
    "react/jsx-uses-react": 1,
    "no-unused-vars": 1,
    "semi": [
      1,
    "always"
    ]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "settings": {
    "version": "latest"
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}
